   },
   "cell_type": "code",
   "source": [
    "def longer_string(x, y):\n",
    "    if len(x) > len(y):\n",
    "        return x\n",
    "    elif len(x) < len(y):\n",
    "        return y\n",
    "    else:\n",
    "        return x\n",
    "\n",
    "longer_string(\"hello\", \"world\")"
   ],
